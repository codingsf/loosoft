<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 数据memcached 地址-->
    <add key="memchached" value="127.0.0.1:11211"/>
    <!-- 线程数量，可以随消息数量的大小进行调整，默认为2个-->
    <add key="thread_num" value="1"/>
    <!-- 线程持久化间隔时间，单位毫秒-->
    <add key="thread_interval" value="10000"/>
    <!-- 是否启动日志-->
    <add key="log.level" value="info"/>
    <!-- 数据上传服务器url地址-->
    <add key="server_url" value="http://spp.foxconn.com/eqcs/EQCService.asmx"/>
    <!-- 程序定时启动间隔（单位分钟）-->
    <add key="restart_interval" value="1"/>
    <!-- 工厂别-->
    <add key="plantId" value="ZZ1"/>
  </appSettings>
</configuration>